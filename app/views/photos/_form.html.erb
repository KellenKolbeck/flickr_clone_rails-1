<%= form_for @photo do |f| %>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label "Upload" %>
    <%= f.file_field :image %>
  </div>

  <%= f.submit "Add Photo", class: "btn btn-success" %>

<% end %>
